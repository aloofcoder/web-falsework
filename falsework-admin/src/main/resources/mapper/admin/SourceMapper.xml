<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="github.aloofcoder.falsework.admin.dao.SourceDao">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="github.aloofcoder.falsework.admin.pojo.entity.SourceEntity">
            <result property="id" column="id"/>
            <result property="sourceName" column="source_name"/>
            <result property="sourceModule" column="source_module"/>
            <result property="method" column="method"/>
            <result property="sourceUrl" column="source_url"/>
            <result property="sourceCode" column="source_code"/>
            <result property="status" column="status"/>
            <result property="createBy" column="create_by"/>
            <result property="editBy" column="edit_by"/>
            <result property="createTime" column="create_time"/>
            <result property="editTme" column="edit_tme"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
    id, source_name, source_module, method, source_url, source_code, status, create_by, edit_by, create_time, edit_tme    </sql>

</mapper>
