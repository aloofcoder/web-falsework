<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="github.aloofcoder.falsework.admin.dao.DictDao">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="github.aloofcoder.falsework.admin.pojo.entity.DictEntity">
            <result property="id" column="id"/>
            <result property="dictName" column="dict_name"/>
            <result property="dictMark" column="dict_mark"/>
